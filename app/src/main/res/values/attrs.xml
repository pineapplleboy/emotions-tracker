<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="QuestionView">
        <attr name="questionText" format="string" />
    </declare-styleable>
    <declare-styleable name="FullscreenAttrs">
        <attr name="fullscreenBackgroundColor" format="color" />
        <attr name="fullscreenTextColor" format="color" />
    </declare-styleable>
    <declare-styleable name="DayMoodSegment">
        <attr name="redPercent" format="float"/>
        <attr name="greenPercent" format="float"/>
        <attr name="bluePercent" format="float"/>
        <attr name="yellowPercent" format="float"/>
        <attr name="period" format="string"/>
        <attr name="amount" format="integer"/>
    </declare-styleable>
</resources>